<div class="panel panel-default">
  <div class="panel-heading">
    <h4>Upload Orders:</h4>
  </div>
  <div class="row">
    <div class="col-sm-5">
      <div class="panel-body">
        <%= form_tag({action: :upload_orders}, multipart: true, role: "form") do %>
            <div class="form-group">
              <%= file_field_tag "orders", class: "form-control" %>
              <br>
              <%= submit_tag("Upload", class: "btn btn-success") %>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
